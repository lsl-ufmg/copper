<div class="gViz-header">
  <div class='symbol'>
    <span class="fa-stack fa-lg">
      <i class="fa fa-circle fa-stack-2x"></i>
      <i class="fa fa-tree fa-stack-1x fa-inverse"></i>
    </span>
  </div>
  <span class='title'>{{title}}</span>
</div>

{{#unless empty}}
  <div class='row'>
    <div class='gViz-wrapper wordtree-diagram' data-id={{_id}} style="overflow: auto;" ></div>
  </div>
{{else}}
  {{visualizations/empty-vis}}
{{/unless}}
