<div class="gViz-header">
  <div class='symbol'>
    <span class="fa-stack fa-lg">
      <i class="fa fa-circle fa-stack-2x"></i>
      <i class="fa fa-list fa-stack-1x fa-inverse"></i>
    </span>
  </div>
  <span class='title'>An√°lise de Sentimentos</span>
</div>

{{#unless empty}}

  <div class='gViz-wrapper sentiment-analysis-wrapper' data-id={{_id}} style={{style}} >
    <div class="controls">
      <div class="gViz-col-1" class="sort-by"> Ordena por: </div>
      <div class="gViz-col-2">
        <select id="order" class="form-control">
          <option value="name">Nome</option>
          <option value="count">Score</option>
        </select>
      </div>
      <div class="gViz-col-9" id="data-buttons"></div>
    </div>
  </div>

{{else}}

  {{visualizations/empty-vis}}

{{/unless}}
